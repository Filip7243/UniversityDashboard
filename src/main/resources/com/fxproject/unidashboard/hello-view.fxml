<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.fxproject.unidashboard.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>

    <TextField fx:id="idInput"/>
    <TextField fx:id="firstNameInput"/>
    <TextField fx:id="secondNameInput"/>
    <TextField fx:id="lastNameInput"/>
    <ListView fx:id="studentsDtoList"/>
    <Button text="Hello!" onAction="#onHelloButtonClick"/>
    <Button text="Find!" onAction="#onFindButtonClick" fx:id="findButton"/>
    <Button text="Find!" onAction="#onAddButtonClick" fx:id="addButton"/>
    <Button text="Find!" onAction="#onRemoveButtonClick" fx:id="removeButton"/>
</VBox>
